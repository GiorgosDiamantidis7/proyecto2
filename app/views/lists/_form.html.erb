<%= simple_form_for list do |f| %>
  <div class="row mb-3">
    <div class="col-md-4">
     <%= f.input :name %>
    </div>
  <div class="col-md-4">
    <%= f.input :date %>
  </div>
  <div class="col-md-4">
    <%= f.input :total %>
  </div>
  <div class="col-md-4 d-flex align-items-end">
    <%= f.submit list.new_record? ? "Crear" : "Editar", class: "btn btn-primary"%>
  </div>
    </div>
  </div>
      <% end %>

